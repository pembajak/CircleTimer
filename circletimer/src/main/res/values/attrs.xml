<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleTimerProgress">
        <attr name="crt_progress" format="integer"/>
        <attr name="crt_max" format="integer"/>

        <attr name="crt_unfinished_color" format="color"/>
        <attr name="crt_finished_color" format="color"/>

        <attr name="crt_finished_stroke_width" format="dimension"/>
        <attr name="crt_unfinished_stroke_width" format="dimension"/>

        <attr name="crt_text_size" format="dimension"/>
        <attr name="crt_text_color" format="color"/>

        <attr name="crt_prefix_text" format="string"/>
        <attr name="crt_suffix_text" format="string"/>
        <attr name="crt_text" format="string"/>

        <attr name="crt_background_color" format="color"/>

        <attr name="crt_inner_bottom_text" format="string"/>
        <attr name="crt_inner_bottom_text_size" format="dimension"/>
        <attr name="crt_inner_bottom_text_color" format="color"/>

        <attr name="crt_circle_starting_degree" format="integer" />
        <attr name="crt_show_text" format="boolean"/>
        <attr name="crt_inner_drawable" format="reference"/>
    </declare-styleable>

</resources>